                                                            <tr>
                                                                <th>Sl No.</th>
                                                                <th>Code</th>
                                                                <th>Title</th>
                                                                <th>Enabled</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>

                                                            <tr id="inds{{$index}}" ng-repeat="x in Categorys">
