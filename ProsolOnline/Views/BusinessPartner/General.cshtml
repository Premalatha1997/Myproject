                                                                <td><label>{{x.Title}}</label></td>
                                                                <td>
                                                                    <label class="switch">
                                                                        <input type="checkbox" ng-model="x.Islive" ng-checked="x.Islive" ng-click="DisableData($index,x.Islive,x._id)">
