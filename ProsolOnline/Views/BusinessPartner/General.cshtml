                                                                    <input type="hidden" ng-model="obj._id" /><input class="form-control immediate-help" name="code" placeholder="Enter Code" ng-model="obj.Code" type="text" required />
                                                                    <div ng-show="form.code.$invalid" class="input-help">
                                                                        <h4 ng-show="form.code.$error.required">Code should not be blank</h4>

                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="mb-md" ng-class="{'has-error': form.$submitted && form.title.$invalid}">
                                                                <div class="controls">
                                                                    <input class="form-control immediate-help" name="title" placeholder="Enter Title" ng-model="obj.Title" type="text" required />
